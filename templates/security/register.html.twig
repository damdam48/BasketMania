{% extends 'base.html.twig' %}

{% block title %}
	S'inscrire
	{{ parent() }}
{% endblock %}

{% block body %}
	<section class="">
		<h1 class="">Inscription</h1>
		{{ form_start(form, {attr: {class: ''}}) }}
		<div class="">
			<div class="">
				{{ form_row(form.firstName) }}
			</div>
			<div class="col-md-6">
				{{ form_row(form.lastName) }}
			</div>
		</div>
		{{ form_widget(form) }}
		<button type="submit" class="">S'inscrire</button>
		{{ form_end(form) }}
	</section>
{% endblock %}
